{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "manifest_version": 3,
  "action": {
    "default_popup": "public/hello.html",
    "default_icon": {
      "16": "public/img/klipp.png"
    }
  }
}
  